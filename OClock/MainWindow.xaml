<Window x:Name="MainWindowDesign" x:Class="OClock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OClock"
        mc:Ignorable="d"
        Title="OClock" Height="450" Width="800" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" BorderThickness="0,0,0,0" SnapsToDevicePixels="True">
    <Grid>
        <StackPanel x:Name="LeftMenu" HorizontalAlignment="Left" Height="400.4" Margin="10,10,0,0" VerticalAlignment="Top" Width="175">
            <TabControl x:Name="Pages" Height="400">
                <TabItem x:Name="CalenderAndEvents" Header="Calender">
                    <Grid Background="#FFE5E5E5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="87*"/>
                            <ColumnDefinition Width="82*"/>
                        </Grid.ColumnDefinitions>
                        <Calendar HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" Width="169" Grid.ColumnSpan="2"/>
                    </Grid>
                </TabItem>
                <TabItem x:Name="ToDoSection" Header="To Do ">
                    <Grid Background="#FFE5E5E5">
                        <ScrollViewer x:Name="ToDoListScrollViewer" HorizontalAlignment="Left" Height="372" Margin="-3,0.04,0,0" VerticalAlignment="Top" Width="172">
                            <StackPanel x:Name="ToDoListStack" Height="372" Width="162">
                                <Button x:Name="ToDoListAddButton" Content="+" HorizontalAlignment="Left" Height="35" Width="152"/>
                            </StackPanel>
                        </ScrollViewer>
                    </Grid>
                </TabItem>
            </TabControl>
        </StackPanel>
        <StackPanel x:Name="RightPanel" HorizontalAlignment="Left" Height="400.4" Margin="200,10,0,0" VerticalAlignment="Top" Width="583.6">
            <StackPanel x:Name="ObservedProcess1" HorizontalAlignment="Left" Height="35" Margin="10,22,0,0" Width="563.6" Orientation="Horizontal">
                <ComboBox x:Name="ProcessSelect" Width="170" IsEditable="True" Margin="0,0,0,0" Padding="5,6,2,2" ScrollViewer.CanContentScroll="True" UseLayoutRounding="False"/>
                <Label x:Name="Time" Content="Time :" Height="35" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center"/>
                <Label Content="HH:" Height="35" VerticalAlignment="Top" Width="30"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <Label Content="MM:" Height="35" VerticalAlignment="Top" Width="35"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <Label Content="SS:" Height="35" VerticalAlignment="Top" Width="30"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <StackPanel x:Name="ButtonsPanel" Height="35" VerticalAlignment="Top" Width="65" Margin="15,0,0,0">
                    <Button x:Name="start" Content="Start" Height="17.5" VerticalAlignment="Top" Background="#FF40F433" Opacity="0.9" Padding="2,-2,1,1" ScrollViewer.CanContentScroll="True" Margin="0" Click="ObserverdProcess1Start"/>
                    <Button x:Name="stop" Content="Stop" Height="17.5" Margin="0" VerticalAlignment="Top" Background="#FFD60C0C" Padding="2,-2,1,1" UseLayoutRounding="True" Opacity="0.9" Click="ObserverdProcess1Stop"/>
                </StackPanel>
            </StackPanel>
            <StackPanel x:Name="ObserverdProcess2" HorizontalAlignment="Left" Height="35" Margin="10,20,0,0" Width="563.6" Orientation="Horizontal">
                <ComboBox x:Name="ProcessSelect_Copy" Width="170" IsEditable="True" Margin="0,0,0,0" Padding="5,6,2,2" ScrollViewer.CanContentScroll="True"/>
                <Label x:Name="Time_Copy" Content="Time :" Height="35" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center"/>
                <Label Content="HH:" Height="35" VerticalAlignment="Top" Width="30"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <Label Content="MM:" Height="35" VerticalAlignment="Top" Width="35"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <Label Content="SS:" Height="35" VerticalAlignment="Top" Width="30"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <StackPanel x:Name="ButtonsPanel_Copy" Height="35" VerticalAlignment="Top" Width="65" Margin="15,0,0,0">
                    <Button x:Name="start1" Content="Start" Height="17.5" VerticalAlignment="Top" Background="#FF40F433" Opacity="0.9" Padding="2,-2,1,1" ScrollViewer.CanContentScroll="True" Margin="0" Click="ObserverdProcess2Start"/>
                    <Button x:Name="stop1" Content="Stop" Height="17.5" Margin="0" VerticalAlignment="Top" Background="#FFD60C0C" Padding="2,-2,1,1" UseLayoutRounding="True" Opacity="0.9" Click="ObserverdProcess2Stop"/>
                </StackPanel>
            </StackPanel>
