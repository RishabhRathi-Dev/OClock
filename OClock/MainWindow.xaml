                <Label Content="SS:" Height="35" VerticalAlignment="Top" Width="30"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <StackPanel x:Name="ButtonsPanel_Copy3" Height="35" VerticalAlignment="Top" Width="65" Margin="15,0,0,0">
                    <Button x:Name="start4" Content="Start" Height="17.5" VerticalAlignment="Top" Background="#FF40F433" Opacity="0.9" Padding="2,-2,1,1" ScrollViewer.CanContentScroll="True" Margin="0" Click="ObserverdProcess5Start"/>
                    <Button x:Name="stop4" Content="Stop" Height="17.5" Margin="0" VerticalAlignment="Top" Background="#FFD60C0C" Padding="2,-2,1,1" UseLayoutRounding="True" Opacity="0.9" Click="ObserverdProcess5Stop"/>
                </StackPanel>
            </StackPanel>
            <StackPanel x:Name="ObserverdProcess6" HorizontalAlignment="Left" Height="35" Margin="10,20,0,0" Width="563.6" Orientation="Horizontal">
                <ComboBox x:Name="ProcessSelect_Copy4" Width="170" IsEditable="True" Margin="0,0,0,0" Padding="5,6,2,2" ScrollViewer.CanContentScroll="True"/>
