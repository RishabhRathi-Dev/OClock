                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <Label Content="SS:" Height="35" VerticalAlignment="Top" Width="30"/>
                <TextBox Height="18" Margin="0,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" TextAlignment="Center" MaxLength="2"/>
                <StackPanel x:Name="ButtonsPanel_Copy4" Height="35" VerticalAlignment="Top" Width="65" Margin="15,0,0,0">
                    <Button x:Name="start5" Content="Start" Height="17.5" VerticalAlignment="Top" Background="#FF40F433" Opacity="0.9" Padding="2,-2,1,1" ScrollViewer.CanContentScroll="True" Margin="0" Click="ObserverdProcess6Start"/>
