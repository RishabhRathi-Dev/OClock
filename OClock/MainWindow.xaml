                    <Button x:Name="start3" Content="Start" Height="17.5" VerticalAlignment="Top" Background="#FF40F433" Opacity="0.9" Padding="2,-2,1,1" Margin="0" Click="ObserverdProcess4Start" ScrollViewer.CanContentScroll="True"/>
                    <Button x:Name="stop3" Content="Stop" Height="17.5" Margin="0" VerticalAlignment="Top" Background="#FFD60C0C" Padding="2,-2,1,1" UseLayoutRounding="True" Opacity="0.9" Click="ObserverdProcess4Stop"/>
                </StackPanel>
            </StackPanel>
            <StackPanel x:Name="ObserverdProcess5" HorizontalAlignment="Left" Height="35" Margin="10,20,0,0" Width="563.6" Orientation="Horizontal">
